@model MvcCv.Models.Entity.socialmedia
@{
    ViewBag.Title = "UpdateSkill";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h4>Sosyal Medya Güncelle</h4>
<br />
<form method="post">
    @Html.Label("Sosyal Medya Adı")
    @Html.TextBoxFor(x => x.name, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.link, new { @class = "form-control" })
    <br />
    @Html.Label("Ikon")
    @Html.TextBoxFor(x => x.icon, new { @class = "form-control" })
    <br />

    <button class="btn btn-success">Güncelle</button>
</form>